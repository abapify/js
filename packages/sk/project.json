{
  "name": "sk",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "sk/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/sk",
        "main": "sk/src/index.ts",
        "tsConfig": "sk/tsconfig.lib.json",
        "assets": ["sk/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "reportsDirectory": "../../coverage/packages/sk",
        "passWithNoTests": true
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
