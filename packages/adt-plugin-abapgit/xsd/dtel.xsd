<?xml version="1.0" encoding="UTF-8"?>
<!--
  abapGit DTEL (Data Element) Schema
  
  Defines DD04V structure for *.dtel.xml files.
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:asx="http://www.sap.com/abapxml"
           elementFormDefault="qualified">

  <xs:import namespace="http://www.sap.com/abapxml" schemaLocation="asx.xsd"/>
  <xs:include schemaLocation="abapgit.xsd"/>

  <!-- DD04V substitutes asx:Schema -->
  <xs:element name="DD04V" type="Dd04vType" substitutionGroup="asx:Schema"/>

  <xs:complexType name="Dd04vType">
    <xs:all>
      <xs:element name="ROLLNAME" type="xs:string"/>
      <xs:element name="DDLANGUAGE" type="xs:string" minOccurs="0"/>
      <xs:element name="DDTEXT" type="xs:string" minOccurs="0"/>
      <xs:element name="DOMNAME" type="xs:string" minOccurs="0"/>
      <xs:element name="DATATYPE" type="xs:string" minOccurs="0"/>
      <xs:element name="LENG" type="xs:string" minOccurs="0"/>
      <xs:element name="DECIMALS" type="xs:string" minOccurs="0"/>
      <xs:element name="HEADLEN" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRLEN1" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRLEN2" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRLEN3" type="xs:string" minOccurs="0"/>
      <xs:element name="REPTEXT" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRTEXT_S" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRTEXT_M" type="xs:string" minOccurs="0"/>
      <xs:element name="SCRTEXT_L" type="xs:string" minOccurs="0"/>
      <xs:element name="DTELMASTER" type="xs:string" minOccurs="0"/>
      <xs:element name="REFKIND" type="xs:string" minOccurs="0"/>
      <xs:element name="ABAP_LANGUAGE_VERSION" type="xs:string" minOccurs="0"/>
    </xs:all>
  </xs:complexType>

</xs:schema>
