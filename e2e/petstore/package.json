{
  "name": "petstore-e2e-example",
  "version": "1.0.0",
  "description": "End-to-end example for ADT CLI export functionality using petstore interface",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "export:dry-run": "adt export package ZPETSTORE ./oat-zpetstore --debug",
    "export:deploy": "adt export package ZPETSTORE ./oat-zpetstore --create --debug",
    "export:deploy-with-transport": "adt export package ZPETSTORE ./oat-zpetstore --create --transport"
  },
  "devDependencies": {
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "abap",
    "adt",
    "petstore",
    "interface",
    "export"
  ],
  "author": "Abapify Team",
  "license": "MIT"
}
