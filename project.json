{
  "name": "abapify",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "local-registry": {
      "executor": "@nx/js:verdaccio",
      "options": {
        "port": 4873,
        "config": ".verdaccio/config.yml",
        "storage": "tmp/local-registry/storage"
      }
    },
    "build": {
      "executor": "nx:noop",
      "dependsOn": []
    },
    "lint": {
      "executor": "nx:noop"
    },
    "test": {
      "executor": "nx:noop"
    },
    "typecheck": {
      "executor": "nx:noop"
    },
    "e2e-ci": {
      "executor": "nx:noop"
    }
  }
}

